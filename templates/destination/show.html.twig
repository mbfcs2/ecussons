{% extends 'base.html.twig' %}

{% block title %}Destination{% endblock %}

{% block body %}
    <h1>Destination</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ destination.id }}</td>
            </tr>
            <tr>
                <th>InseeCode</th>
                <td>{{ destination.inseeCode }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ destination.zipCode }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ destination.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ destination.slug }}</td>
            </tr>
            <tr>
                <th>GpsLat</th>
                <td>{{ destination.gpsLat }}</td>
            </tr>
            <tr>
                <th>GpsLng</th>
                <td>{{ destination.gpsLng }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('destination_index') }}">back to list</a>

    <a href="{{ path('destination_edit', {'id': destination.id}) }}">edit</a>

    {{ include('destination/_delete_form.html.twig') }}
